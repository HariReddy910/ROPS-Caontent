                            <option>abc</option>
                            <option>5</option>
                        </select>
                    </div>
                </div>
           <hr>
                    <div class="form-group">
                        <label for="inputtext3" class="col-sm-4 control-label">Search By Expense</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="inputtext3">
                        </div>
                    </div>
                <button type="button" class="btn btn-success" style="margin-left: 35%;"><i class="fa fa-floppy-o fa-2x" aria-hidden="true"></i> Save.</button>
                    
                </form>

